{
    "name": "danineto/annotations",
    "description": "Annotations are a mechanism for adding metadata information to source code.",
    "type": "library",
    "license": "MIT",
    "support": {
        "source": "https://github.com/danineto/annotations",
        "issues": "https://github.com/danineto/annotations/issues"
    },
    "authors": [
        {
            "name": "Daniel Neto",
            "email": "daniel.neto89@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/annotations": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Danineto\\Annotations\\": "src/"
        },
        "files": [
            "bootstrap.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "stable",
}